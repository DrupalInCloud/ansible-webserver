if ($http_host ~* "^www\.maks-portal\.ru$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^maks-portal\.contorra\.ru$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^www\.maks-portal\.contorra\.ru$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
