    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^www\.maksportal\.com$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^www\.maks-portal\.com$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^maksportal\.ru$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^maksportal\.com$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^maks-portal\.com$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^sbc-sochi\.ru$"){
    rewrite ^(.*)$ http://maks-portal.ru/sochinskii-delovoi-klub permanent;
  }
  if ($http_host ~* "^www\.sbc-sochi\.ru$"){
    rewrite ^(.*)$ http://maks-portal.ru/sochinskii-delovoi-klub permanent;
  }
  if ($http_host ~* "^www\.xn----7sbb2bhbrhkgp\.xn--p1ai$"){
