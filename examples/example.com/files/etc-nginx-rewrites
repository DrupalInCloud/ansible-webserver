  if ($http_host ~* "^xn----7sbb2bhbrhkgp\.xn--p1ai$"){
    rewrite ^(.*)$ http://maks-portal.ru$1 permanent;
  }
  if ($http_host ~* "^www\.xn--80aa3afbogjgn\.xn--p1ai$"){
