/home/webmaster/domains/*/logs/*nginx*.log {
        daily           
        missingok
        rotate 20
        compress
        delaycompress
        notifempty
        create 664 www-data root
        sharedscripts
        postrotate
            [ ! -f /var/run/nginx.pid ] || kill -HUP `cat /var/run/nginx.pid`
        endscript
}