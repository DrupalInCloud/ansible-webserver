/home/webmaster/domains/*/logs/*nginx*.log {
        daily           
        missingok
        rotate 20
        compress
        delaycompress
        notifempty
        create 640 webmaster webmaster
        sharedscripts
        postrotate
            [ ! -f /var/run/nginx.pid ] || kill -USR1 `cat /var/run/nginx.pid`
        endscript
}